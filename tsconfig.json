{
  "compilerOptions": {
    "noImplicitAny": true,
    "sourceMap": true,
    "allowJs": true,
    "target": "es5",
    "importHelpers": true,
    "lib": [
      "esnext.asynciterable",
      "es2015",
      "es2017",
      "dom"
    ],
    "typeRoots": [
      "./node_modules/@types",
      "./src/js/@types"
    ],
    "jsx": "react"
  },
  "exclude": [
    "node_modules",
    "functions"
  ]
}
